{
  "hosting": {
    "source": "https://github.com/<your-github-username>/<your-repo-name>.git",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/templates/index.html"
      }
    ]
  }
}
